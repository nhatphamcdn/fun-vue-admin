{
  "name": "fun-vue-admin",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite --mode serve-dev --host",
    "test": "vite --mode serve-test --host",
    "build": "vite build",
    "preview": "vite preview",
    "prepare": "husky install",
    "typecheck": "vue-tsc --noEmit",
    "format": "prettier --write ./src ./cypress",
    "eslint:debug": "eslint --debug --ext .js,.ts,.vue src cypress",
    "eslint:fix": "eslint --ext .js,.ts,.vue src cypress --fix",
    "test:component": "npx cypress run --component"
  },
  "dependencies": {
    "pinia": "^2.0.17",
    "vue": "^3.2.37",
    "vue-router": "^4.1.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@nhatphamfks-org/funstyle": "^1.0.1",
    "@types/node": "^18.6.1",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "@vitejs/plugin-vue": "^3.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "cypress": "^10.3.1",
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.3.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "sass": "^1.54.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.6.4",
    "vite": "^3.0.0",
    "vue-eslint-parser": "^9.0.3",
    "vue-tsc": "^0.38.4"
  },
  "lint-staged": {
    "*.{js,vue,ts}": [
      "prettier --write"
    ],
    "*.{scss, css, md}": [
      "prettier --write"
    ]
  }
}
